    "@types/react-test-renderer": "^18.0.0",
    "@types/node": "^20.10.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "2.8.8",
